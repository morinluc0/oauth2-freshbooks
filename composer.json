{
  "name": "zerospam/oauth2-freshbook",
  "description": "Freshbook OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Antoine Aflalo",
      "email": "antoine.aflalo@zerospam.ca",
      "homepage": "https://www.zerospam.ca"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "freshbook"
  ],
  "require": {
    "league/oauth2-client": "^2.0"
  },
  "require-dev": {
    "php": ">= 7",
    "mockery/mockery": "^1.1",
    "squizlabs/php_codesniffer": "^3.2",
    "phpunit/phpunit": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "ZEROSPAM\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZEROSPAM\\OAuth2\\Client\\Test\\": "tests/src/"
    }
  }
}
